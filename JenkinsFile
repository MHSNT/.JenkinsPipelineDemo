            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Building'
            }
         }
        stage('Dev') {
            steps {
                echo 'Develop'
            }
          }
         stage('Test') {
            steps {
                echo 'Testing'
            }
            }
            stage('Release') {
            steps {
                echo 'Releasing'
                }
            }
        }
}
